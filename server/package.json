{
  "name": "passeios-turisticos-bba",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "keywords": [
    "node",
    "express",
    "sequelize",
    "postgres"
  ],
  "private": true,
  "license": "MIT",
  "main": "/src/server.js",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanabacco/passeios-turisticos-bba"
  },
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
    "test": "npm run db:reset && jest --detectOpenHandles --colors",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src --fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
