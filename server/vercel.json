{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/server.js"
    }
  ],
  "headers": [
    {
      "source": "/api-docs/swagger-ui.css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    }
  ]
}
