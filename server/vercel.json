{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/swagger-config.json",
      "dest": "/swagger-config.json"
    },
    {
      "src": "/index.css",
      "dest": "/index.css"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}
